@model Web_dienthoai.Models.SanPham
@{
    ViewBag.Title = "HoiXoa";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div style="margin:60px">
    <h2 style="text-align:center">Chắc chắn xóa sản phẩm :@Model.TenSP?</h2>
    <p style="color:red">@ViewBag.thongbao</p>
    <p style="text-align:center">
        @Html.ActionLink("Xóa", "XoaSP","QuanLySanPham", new { id = Model.MaSP }, new { @class = "btn btn-primary" })
        @Html.ActionLink("Hủy", "Index", null, new { @class = "btn btn-success" })
    </p>


</div>


